{
  "name": "slc",
  "version": "0.0.62",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "serve": "cd dist/web && serve",
    "test": "ts-node-esm  -T ../../../script.ts/cmd.test.mts",
    "build": "ts-node-esm -T ../../../script.ts/cmd.build.mts",
    "clean": "ts-node-esm -T ../../../script.ts/cmd.clean.mts",
    "bundle": "ts-node-esm -T ./script/bundle.mts",
    "patch": "yarn version --patch --no-git-tag-version",
    "prepare": "yarn build && yarn test --run --reporter=verbose && yarn bundle",
    "deploy": "yarn audit && yarn patch && yarn prepare && ts-node-esm -T ./script/deploy.mts",
    "tmp": "ts-node-esm -T ./script/tmp.mts"
  },
  "dependencies": {
    "ext.ui.react.vimeo": "0.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sys.data.crdt": "0.0.0",
    "sys.text": "0.0.0",
    "sys.ui.react.common": "0.0.0",
    "sys.ui.react.concept": "0.0.0",
    "sys.util": "0.0.0"
  },
  "devDependencies": {
    "sys.test": "0.0.0",
    "sys.test.spec": "0.0.0",
    "sys.types": "0.0.0"
  },
  "license": "MIT",
  "private": true,
  "types": "./dist/types.d/index.d.mts",
  "exports": {
    ".": "./dist/web/index.js",
    "./specs": "./dist/web/specs.js"
  },
  "typesVersions": {
    "*": {
      "specs": [
        "./dist/types.d/test.ui/entry.Specs.d.mts"
      ]
    }
  }
}
