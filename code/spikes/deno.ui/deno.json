{
  "name": "@tdb/foo",
  "version": "0.0.16",
  "description": "Sample",
  "main": "./src/mod.ts",
  "author": "Phil Cockfield",
  "repository": "https://github.com/cellplatform/platform-0.2.0",
  "license": "MIT",
  "tasks": {
    "start": "deno run -RN ./src/start.ts",
    "dev": "deno run -RN --check --watch ./src/start.ts",
    "check": "deno check ./src/mod.ts",
    "test": "deno test --watch",
    "prepare": "deno run -RW ./script/prepare.ts",
    "pub": "deno task prepare && deno publish --allow-dirty",
    "pub-dry": "deno task prepare && deno publish --allow-dirty --dry-run"
  },
  "exports": {
    ".": "./src/mod.ts",
    "./mod.ts": "./src/mod.ts"
  },
  "publish": {
    "exclude": ["!dist", "script"]
  }
}
